{
    "label" : "logout",
    "action" : "loginctl kill-session $XDG_SESSION_ID",
    "text" : "        "     
    "keybind" : "e"
}

{
    "label" : "lock",
    "action" : "$HOME/.config/hypr/scripts/LockScreen.sh",
    "text" : "        "
    "keybind" : "l"
}


{
    "label" : "shutdown",
    "action" : "systemctl poweroff",
    "text" : "        "
    "keybind" : "s"
}

{
    "label" : "reboot",
    "action" : "systemctl reboot",
    "text" : "        "     
    "keybind" : "r"
}

{
    "label" : "suspend",
    "action" : "systemctl suspend",
